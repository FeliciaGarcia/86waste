        <div class="card-image">
          <img style="width:375px; height:300px;" ng-src="{{ $ctrl.post.photourl }}">
        </div>
        <div class="card-content">
          <p>{{ $ctrl.post.content }}</p>
         </div>
         <div class="crud-buttons">
         <a ui-sref="posts"><button class="btn btn-default blue text-white" style="margin: 10px; " >back to posts</button></a>
         <a><button class="btn btn-default orange accent-4 text-white" style="margin: 10px"; ui-sref="postsEdit({id: $ctrl.post.id})">Edit</button></a>
         <button style="margin: 10px;" class="btn btn-default red text-white" ng-click='$ctrl.postDelete($ctrl.post.id)'>Delete</button>
         </div>
        </div>
      </div>
      </div>
    </div>

  </div>

